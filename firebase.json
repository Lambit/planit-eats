{
  "react-native": {
    "android_task_executor_maximum_pool_size": 10,
    "android_task_executor_keep_alive_seconds": 3
  },
  "functions": {
    "deploy": [
      "firebase deploy --only functions"
    ],
    "source": "functions"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    },
    "hosting": {
      "port": 5000
    }
  },
  "hosting": {
    "public": "src/App.js",
    "ignore": [
      "*"
    ]
  }
}

